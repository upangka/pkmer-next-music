{
  "name": "@pkmer-music-ui/react",
  "version": "1.0.0",
  "type": "module",
  "description": "Music React UI Components",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "rollup -c -w && tsc --project tsconfig.json",
    "build": "rollup -c && tsc --project tsconfig.json"
  },
  "dependencies": {
    "@pkmer/libs": "workspace:*"
  },
  "peerDependencies": {
    "react": "catalog:react19",
    "react-dom": "catalog:react19"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "esbuild": "^0.24.2",
    "rollup": "^4.29.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.7.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./avatar": {
      "import": {
        "types": "./dist/types/components/avatar/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./upload-status": {
      "import": {
        "types": "./dist/types/components/upload-status/index.d.ts",
        "default": "./dist/index.js"
      }
    }
